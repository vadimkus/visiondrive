{
  "name": "visiondrive",
  "version": "1.0.0",
  "description": "Smart kitchen temperature monitoring platform for UAE commercial kitchens",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3000",
    "clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "build": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\" && prisma generate && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "framer-motion": "^12.34.3",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.2.0",
    "lucide-react": "^0.575.0",
    "mapbox-gl": "^3.18.1",
    "next": "^16.1.6",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^10.0.1",
    "eslint-config-next": "^16.1.6",
    "png-to-ico": "^3.0.1",
    "prisma": "^7.4.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=9.0.0"
  }
}
