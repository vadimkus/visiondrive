{
  "name": "visiondrive",
  "version": "1.0.0",
  "description": "Smart parking solution platform for the UAE",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "build": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\" && prisma generate && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "fetch:2gis": "node scripts/fetch-2gis-parking.js",
    "import:2gis": "tsx scripts/import-2gis-parking.js"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^10.18.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.555.0",
    "mapbox-gl": "^3.17.0",
    "next": "^16.0.7",
    "postgres": "^3.4.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.4.32",
    "prisma": "^7.1.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=9.0.0"
  }
}
